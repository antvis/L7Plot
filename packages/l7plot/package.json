{
  "name": "@antv/l7plot",
  "version": "0.0.1-alpha.1",
  "description": "A map charting library",
  "main": "dist/lib/index.js",
  "types": "dist/lib/index.d.ts",
  "module": "dist/esm/index.js",
  "unpkg": "dist/umd/index.js",
  "scripts": {
    "dev": "npm run clean && npm run dev:cjs",
    "dev:cjs": "tsc -p tsconfig.json --target ES5 --module CommonJS --outDir dist/lib --watch",
    "lint": "eslint src __tests__ --ext .js,.ts",
    "lint-fix": "eslint src __tests__ --ext .js,.ts --fix",
    "prettier": "prettier src __tests__ -c -w",
    "typedoc": "typedoc",
    "clean": "rimraf dist",
    "build": "npm run clean && npm run build:cjs && npm run build:esm",
    "build:cjs": "tsc -p tsconfig.json --target ES5 --module CommonJS --outDir dist/lib",
    "build:esm": "tsc -p tsconfig.json --target ES6 --module ESNext --outDir dist/esm",
    "build:umd": "tsc -p tsconfig.json --target ES5 --module UMD --outDir dist/umd",
    "rollup": "rollup -c rollup.config.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/liuvigongzuoshi/L7Plot.git"
  },
  "keywords": [
    "antv",
    "chart",
    "l7",
    "plot"
  ],
  "author": "yunji",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/liuvigongzuoshi/L7Plot/issues"
  },
  "homepage": "https://github.com/liuvigongzuoshi/L7Plot#readme",
  "devDependencies": {
    "rimraf": "^3.0.2",
    "rollup": "^2.52.2",
    "typedoc": "^0.21.0"
  },
  "dependencies": {
    "@antv/event-emitter": "^0.1.2",
    "@antv/l7": "^2.4.3",
    "@antv/util": "^2.0.13"
  }
}
