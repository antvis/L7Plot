{
  "name": "l7plot",
  "private": true,
  "author": "yunji",
  "license": "MIT",
  "scripts": {
    "prepare": "husky install",
    "bootstrap": "lerna bootstrap --hoist",
    "dev": "lerna run dev --stream --parallel --ignore=l7plot-website",
    "dev-storybook": "lerna run dev --stream --scope=l7plot-storybook",
    "dev-website": "lerna run dev --stream --scope=l7plot-website",
    "lint": "lerna run lint",
    "lint-fix": "lerna run lint-fix",
    "prettier": "lerna run prettier",
    "test": "jest",
    "test-cover": "jest --coverage",
    "build": "lerna run build",
    "build-l7lot": "lerna run build --stream --scope=@antv/l7plot",
    "build-website": "lerna run build --stream --scope=l7plot-website"
  },
  "devDependencies": {
    "@commitlint/cli": "^12.1.4",
    "@commitlint/config-conventional": "^12.1.4",
    "@types/jest": "^26.0.23",
    "@typescript-eslint/eslint-plugin": "^4.27.0",
    "@typescript-eslint/parser": "^4.27.0",
    "eslint": "^7.29.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-jest": "^24.3.6",
    "eslint-plugin-prettier": "^3.4.0",
    "husky": "^6.0.0",
    "jest": "^27.0.4",
    "lerna": "^4.0.0",
    "lint-staged": "^11.0.0",
    "prettier": "^2.3.1",
    "ts-jest": "^27.0.3",
    "typescript": "^4.3.4"
  }
}
